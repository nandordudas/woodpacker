{
  "name": "workspace/woodpacker",
  "type": "project",
  "license": "proprietary",
  "scripts": {
    "dev": "php -S localhost:8080 -t ./public"
  },
  "authors": [
    {
      "name": "Nandor Dudas",
      "email": "nandor.dudas@gmail.com"
    },
    {
      "name": "Csaba Nagy",
      "email": "dev.csaba.nagy@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.2",
    "ext-pdo": "*",
    "psr/http-message": "^2.0"
  },
  "require-dev": {
    "filp/whoops": "^2.15",
    "symfony/var-dumper": "^6.2"
  },
  "autoload": {
    "psr-4": {
      "Woodpacker\\": "src/"
    }
  },
  "minimum-stability": "stable",
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
